<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <!--  <constant name="struts.devMode" value="true"/>-->
    <package name="struts" extends="struts-default">
        <!--登录配置-->
        <action name="loginAction_*" class="loginAction" method="{1}">
            <result name="login">/login.jsp</result>
            <result name="success">/index.jsp</result>
            <result name="reg">/reg.jsp</result>
            <result name="ret">/index.jsp</result>
            <result name="reg1">/reg.jsp</result>
            <result name="login1">/login.jsp</result>
            <result name="play">/2048.jsp</result>
            <result name="review">/carreviews.jsp</result>
            <result name="ipa">/I-PACE.jsp</result>
            <result name="bl">/bently.jsp</result>
            <result name="us">/Urus.jsp</result>
            <result name="mre">/martinRE.jsp</result>
            <allowed-methods>login,reg,ret,reg1,login1,success,play,review,ipa,bl,us,mre</allowed-methods>
        </action>
    </package>
</struts>